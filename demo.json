// 这只是一个示例问卷，使用时请根据实际情况修改并删除注释以避免 JSON 语法错误
{
  "main_title": "示例问卷",              // 问卷主标题
  "sub_title": "这是一个示例问卷",       // 问卷副标题
  "score": 30,                          // 问卷总分
  "pass": 15,                          // 及格分数线
  "isInGroup": "True",                 // 是否需要加群验证
  "isTest": "False",                   // 是否为测试模式
  "client_key": "your_key_here",       // 客户端密钥，自托管模式下无需填写
  "questions": [
    {
      "type": "radio",             // 单选题
      "content": "这是一个单选题示例？",
      "options": [                  // 选项数组
        "选项A",
        "选项B",
        "选项C",
        "选项D"
      ],
      "correct": 1,                // 正确答案索引（从1开始）
      "score": 2                   // 本题分值
    },
    {
      "type": "multiple",          // 多选题
      "content": "这是一个多选题示例？",
      "options": [                 // 选项数组
        "选项A",
        "选项B",
        "选项C",
        "选项D"
      ],
      "correct": [1, 2],          // 正确答案索引数组（从1开始）
      "score": [1, 2]             // 部分正确和全部正确的分值
    }
  ]
}
